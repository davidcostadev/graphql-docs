  "dependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "graphql": "^16.9.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "http-server": "^14.1.1",
    "isomorphic-fetch": "^3.0.0",
    "tsx": "^4.20.4"
  }
}
